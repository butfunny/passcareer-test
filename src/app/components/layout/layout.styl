.layout {
  position relative;
  padding-left 65px;
  min-height 100vh;
  padding-top: 65px;
  background #F4F5F8;

  .header {
    position absolute;
    top: 0;
    left 0;
    width 100%;
    height 65px;
    border-bottom 1px solid #dedede;
    background white;
  }


  .menu-item {
    position absolute;
    top: 65px;
    left 0;
    width 65px;
    height calc(100vh - 65px);
    z-index 2;
  }

  .container {
    z-index 1;
  }
}
